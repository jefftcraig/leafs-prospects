{
  "extends": ["airbnb"],
  "rules": {
    "arrow-body-style": 0,
    "camelcase": 0,
    "no-script-url": 0,
    "no-constant-condition": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-unneeded-ternary": 0,
    "no-nested-ternary": 0,
    "react/no-multi-comp": 0,
    "max-len": [
      2,
      { 
        "code": 150,
        "ignoreTrailingComments": true,
        "ignoreUrls": true,
        "ignoreStrings": true
      }
    ],
    "no-unused-vars": [
      1, 
      {
        "vars": "local",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "globals": {
    "document": true,
    "window": true,
    "$": true,
    "jQuery": true,
    "jquery": true,
    "metismenu": true,
    "bootstrap": true,
    "FileReader": true,
    "FormData": true,
    "parseInt": true
  }
}